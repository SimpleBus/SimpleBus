{
    "name": "simple-bus/rabbitmq-bundle-bridge",
    "type": "symfony-bundle",
    "description": "Use OldSoundRabbitMQBundle with SimpleBus/AsynchronousBundle",
    "keywords": [
        "RabbitMQ",
        "messages",
        "message bus",
        "event bus",
        "command bus"
    ],
    "homepage": "http://github.com/SimpleBus/RabbitMQBundleBridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Cliff Odijk",
            "email": "cliff@jcid.nl"
        },
        {
            "name": "Ruud Kamphuis",
            "homepage": "https://github.com/ruudk"
        },
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "beberlei/assert": "^3.0",
        "emag-tech-labs/rabbitmq-bundle": "^2.3",
        "simple-bus/asynchronous": "^3.0",
        "simple-bus/asynchronous-bundle": "^3.0",
        "simple-bus/message-bus": "^3.0.3",
        "symfony/monolog-bundle": "^3.4"
    },
    "require-dev": {
        "matthiasnoback/phpunit-asynchronicity": "^2.0",
        "phpunit/phpunit": "^9.3",
        "simple-bus/jms-serializer-bundle-bridge": "~3.0",
        "symfony/console": "^4.4 || ^5.1",
        "symfony/finder": "^4.4 || ^5.1",
        "symfony/process": "^4.4 || ^5.1",
        "symfony/translation": "^4.4 || ^5.1"
    },
    "autoload": {
        "psr-4": {
            "SimpleBus\\RabbitMQBundleBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleBus\\RabbitMQBundleBridge\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
